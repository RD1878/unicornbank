{
	"env": {
	  "browser": true,
	  "es6": true
	},
	"extends": [
	  "plugin:react/recommended",
	  "plugin:@typescript-eslint/recommended",
	  "plugin:prettier/recommended"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
	  "ecmaFeatures": {
		"jsx": true
	  },
	  "ecmaVersion": 2018,
	  "sourceType": "module"
	},
	"plugins": ["react", "@typescript-eslint"],
  "rules": { "no-console": 1 },
  "overrides": [
    {
      "files": ["**/*.tsx"],
      "rules": {
        "react/prop-types": "off"
      }
    }
  ]
}
